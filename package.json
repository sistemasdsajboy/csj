		"husky": "^9.1.5",
		"lodash": "^4.17.21",
		"lucia": "^3.2.0",
		"node-xlsx": "^0.24.0",
		"playwright": "^1.46.1",
		"postcss": "^8.4.44",
		"prettier": "^3.3.3",
		"prettier-plugin-svelte": "^3.2.6",
		"prettier-plugin-tailwindcss": "^0.6.6",
		"resend": "^4.0.0",
		"svelte": "^5.0.0-next.243",
		"svelte-check": "^4.0.0",
		"tailwindcss": "^3.4.10",
