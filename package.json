		"@tailwindcss/typography": "^0.5.15",
		"@types/eslint": "^9.6.1",
		"@types/file-saver": "^2.0.7",
		"@types/lodash": "^4.17.7",
		"@types/node": "^20.16.3",
		"@typescript-eslint/eslint-plugin": "^8.4.0",
		"@typescript-eslint/parser": "^8.4.0",
		"autoprefixer": "^10.4.20",
		"dayjs": "^1.11.13",
		"eslint": "^9.9.1",
		"eslint-config-prettier": "^9.1.0",
		"eslint-plugin-svelte": "^2.43.0",
		"file-saver": "^2.0.5",
